SRC=/home/denis/reps/denis-belov/xgk-test/src
BUILD=/home/denis/reps/denis-belov/xgk-test/build/gcc-x64
LIBS=/home/denis/libs
REPS=/home/denis/reps
MAKE_H=mkdir -p $(BUILD)/h
MAKE_SPV=mkdir -p $(BUILD)/spv

$(BUILD)/output/bin/xgk-test.bin : $(BUILD)/internal/o/xgk-test.o $(BUILD)/internal/o/gl.o $(BUILD)/internal/o/vk.o $(BUILD)/external/o/$(LIBS)/glad/src/glad.o $(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui.o $(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui_widgets.o $(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui_draw.o $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_glfw.o $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_opengl3.o $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_vulkan.o $(LIBS)/glfw-3.3.2/src/libglfw3.a $(REPS)/denis-belov/xgk-math/build/gcc-x64/output/a/xgk-math.a $(REPS)/denis-belov/xgk-aux/build/gcc-x64/output/a/xgk-aux.a
	mkdir -p $(BUILD)/output/bin && mkdir -p $(BUILD)/output/s && g++ $(BUILD)/internal/o/xgk-test.o $(BUILD)/internal/o/gl.o $(BUILD)/internal/o/vk.o $(BUILD)/external/o/$(LIBS)/glad/src/glad.o $(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui.o $(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui_widgets.o $(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui_draw.o $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_glfw.o $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_opengl3.o $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_vulkan.o $(LIBS)/glfw-3.3.2/src/libglfw3.a $(REPS)/denis-belov/xgk-math/build/gcc-x64/output/a/xgk-math.a $(REPS)/denis-belov/xgk-aux/build/gcc-x64/output/a/xgk-aux.a -l dl -l pthread -l Xrandr -l Xxf86vm -l Xi -l Xinerama -l Xcursor -flto -o $(BUILD)/output/bin/xgk-test.bin && objdump -d -M intel -S $(BUILD)/output/bin/xgk-test.bin > $(BUILD)/output/s/xgk-test.s

$(BUILD)/internal/o/xgk-test.o : $(SRC)/xgk-test.cpp 
	mkdir -p $(BUILD)/internal/o && mkdir -p $(BUILD)/internal/s && g++ $(SRC)/xgk-test.cpp -c -std=c++20 -m64 -msse3 -Ofast -funroll-loops -Wall -Wextra -Wpedantic -Wno-cpp -I $(SRC) -I $(LIBS) -I $(LIBS)/glad/include -I $(REPS)/ocornut/imgui -I $(LIBS)/glfw-3.3.2/include -I $(REPS)/denis-belov -I $(REPS)/denis-belov/xgk-test/build/gcc-x64/h -I $(REPS)/denis-belov/xgk-math/src -o $(BUILD)/internal/o/xgk-test.o && objdump -d -M intel -S $(BUILD)/internal/o/xgk-test.o > $(BUILD)/internal/s/xgk-test.s

$(BUILD)/internal/o/gl.o : $(SRC)/gl.cpp 
	mkdir -p $(BUILD)/internal/o && mkdir -p $(BUILD)/internal/s && g++ $(SRC)/gl.cpp -c -std=c++20 -m64 -msse3 -Ofast -funroll-loops -Wall -Wextra -Wpedantic -Wno-cpp -I $(SRC) -I $(LIBS) -I $(LIBS)/glad/include -I $(REPS)/ocornut/imgui -I $(LIBS)/glfw-3.3.2/include -I $(REPS)/denis-belov -I $(REPS)/denis-belov/xgk-test/build/gcc-x64/h -I $(REPS)/denis-belov/xgk-math/src -o $(BUILD)/internal/o/gl.o && objdump -d -M intel -S $(BUILD)/internal/o/gl.o > $(BUILD)/internal/s/gl.s

$(BUILD)/internal/o/vk.o : $(SRC)/vk.cpp $(REPS)/denis-belov/xgk-aux/src/api/vulkan.h $(BUILD)/h/vertex_shader_code_vulkan.h $(BUILD)/h/fragment_shader_code_vulkan.h
	mkdir -p $(BUILD)/internal/o && mkdir -p $(BUILD)/internal/s && g++ $(SRC)/vk.cpp -c -std=c++20 -m64 -msse3 -Ofast -funroll-loops -Wall -Wextra -Wpedantic -Wno-cpp -I $(SRC) -I $(LIBS) -I $(LIBS)/glad/include -I $(REPS)/ocornut/imgui -I $(LIBS)/glfw-3.3.2/include -I $(REPS)/denis-belov -I $(REPS)/denis-belov/xgk-test/build/gcc-x64/h -I $(REPS)/denis-belov/xgk-math/src -o $(BUILD)/internal/o/vk.o && objdump -d -M intel -S $(BUILD)/internal/o/vk.o > $(BUILD)/internal/s/vk.s

$(BUILD)/h/vertex_shader_code_vulkan.h : $(SRC)/vk-shaders/vertex_shader_code_vulkan.vert
	$(MAKE_SPV) && $(MAKE_H) && glslangValidator -o $(BUILD)/spv/vertex_shader_code_vulkan.spv $(SRC)/vk-shaders/vertex_shader_code_vulkan.vert -V -t && node $(REPS)/denis-belov/c-build/src/spv2h $(BUILD)/spv/vertex_shader_code_vulkan.spv $(BUILD)/h/vertex_shader_code_vulkan.h vertex_shader_code_vulkan

$(BUILD)/h/fragment_shader_code_vulkan.h : $(SRC)/vk-shaders/fragment_shader_code_vulkan.frag
	$(MAKE_SPV) && $(MAKE_H) && glslangValidator -o $(BUILD)/spv/fragment_shader_code_vulkan.spv $(SRC)/vk-shaders/fragment_shader_code_vulkan.frag -V -t && node $(REPS)/denis-belov/c-build/src/spv2h $(BUILD)/spv/fragment_shader_code_vulkan.spv $(BUILD)/h/fragment_shader_code_vulkan.h fragment_shader_code_vulkan

$(BUILD)/external/o/$(LIBS)/glad/src/glad.o : $(LIBS)/glad/src/glad.c 
	mkdir -p $(BUILD)/external/o/$(LIBS)/glad/src && mkdir -p $(BUILD)/external/s/$(LIBS)/glad/src && gcc $(LIBS)/glad/src/glad.c -c -m64 -msse3 -Ofast -funroll-loops -Wall -Wextra -Wpedantic -I $(SRC) -I $(LIBS) -I $(LIBS)/glad/include -I $(REPS)/ocornut/imgui -I $(LIBS)/glfw-3.3.2/include -I $(REPS)/denis-belov -I $(REPS)/denis-belov/xgk-test/build/gcc-x64/h -I $(REPS)/denis-belov/xgk-math/src -o $(BUILD)/external/o/$(LIBS)/glad/src/glad.o && objdump -d -M intel -S $(BUILD)/external/o/$(LIBS)/glad/src/glad.o > $(BUILD)/external/s/$(LIBS)/glad/src/glad.s

$(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui.o : $(REPS)/ocornut/imgui/imgui.cpp 
	mkdir -p $(BUILD)/external/o/$(REPS)/ocornut/imgui && mkdir -p $(BUILD)/external/s/$(REPS)/ocornut/imgui && g++ $(REPS)/ocornut/imgui/imgui.cpp -c -std=c++20 -m64 -msse3 -Ofast -funroll-loops -Wall -Wextra -Wpedantic -Wno-cpp -I $(SRC) -I $(LIBS) -I $(LIBS)/glad/include -I $(REPS)/ocornut/imgui -I $(LIBS)/glfw-3.3.2/include -I $(REPS)/denis-belov -I $(REPS)/denis-belov/xgk-test/build/gcc-x64/h -I $(REPS)/denis-belov/xgk-math/src -o $(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui.o && objdump -d -M intel -S $(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui.o > $(BUILD)/external/s/$(REPS)/ocornut/imgui/imgui.s

$(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui_widgets.o : $(REPS)/ocornut/imgui/imgui_widgets.cpp 
	mkdir -p $(BUILD)/external/o/$(REPS)/ocornut/imgui && mkdir -p $(BUILD)/external/s/$(REPS)/ocornut/imgui && g++ $(REPS)/ocornut/imgui/imgui_widgets.cpp -c -std=c++20 -m64 -msse3 -Ofast -funroll-loops -Wall -Wextra -Wpedantic -Wno-cpp -I $(SRC) -I $(LIBS) -I $(LIBS)/glad/include -I $(REPS)/ocornut/imgui -I $(LIBS)/glfw-3.3.2/include -I $(REPS)/denis-belov -I $(REPS)/denis-belov/xgk-test/build/gcc-x64/h -I $(REPS)/denis-belov/xgk-math/src -o $(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui_widgets.o && objdump -d -M intel -S $(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui_widgets.o > $(BUILD)/external/s/$(REPS)/ocornut/imgui/imgui_widgets.s

$(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui_draw.o : $(REPS)/ocornut/imgui/imgui_draw.cpp 
	mkdir -p $(BUILD)/external/o/$(REPS)/ocornut/imgui && mkdir -p $(BUILD)/external/s/$(REPS)/ocornut/imgui && g++ $(REPS)/ocornut/imgui/imgui_draw.cpp -c -std=c++20 -m64 -msse3 -Ofast -funroll-loops -Wall -Wextra -Wpedantic -Wno-cpp -I $(SRC) -I $(LIBS) -I $(LIBS)/glad/include -I $(REPS)/ocornut/imgui -I $(LIBS)/glfw-3.3.2/include -I $(REPS)/denis-belov -I $(REPS)/denis-belov/xgk-test/build/gcc-x64/h -I $(REPS)/denis-belov/xgk-math/src -o $(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui_draw.o && objdump -d -M intel -S $(BUILD)/external/o/$(REPS)/ocornut/imgui/imgui_draw.o > $(BUILD)/external/s/$(REPS)/ocornut/imgui/imgui_draw.s

$(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_glfw.o : $(REPS)/ocornut/imgui/examples/imgui_impl_glfw.cpp 
	mkdir -p $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples && mkdir -p $(BUILD)/external/s/$(REPS)/ocornut/imgui/examples && g++ $(REPS)/ocornut/imgui/examples/imgui_impl_glfw.cpp -c -std=c++20 -m64 -msse3 -Ofast -funroll-loops -Wall -Wextra -Wpedantic -Wno-cpp -I $(SRC) -I $(LIBS) -I $(LIBS)/glad/include -I $(REPS)/ocornut/imgui -I $(LIBS)/glfw-3.3.2/include -I $(REPS)/denis-belov -I $(REPS)/denis-belov/xgk-test/build/gcc-x64/h -I $(REPS)/denis-belov/xgk-math/src -o $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_glfw.o && objdump -d -M intel -S $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_glfw.o > $(BUILD)/external/s/$(REPS)/ocornut/imgui/examples/imgui_impl_glfw.s

$(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_opengl3.o : $(REPS)/ocornut/imgui/examples/imgui_impl_opengl3.cpp 
	mkdir -p $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples && mkdir -p $(BUILD)/external/s/$(REPS)/ocornut/imgui/examples && g++ $(REPS)/ocornut/imgui/examples/imgui_impl_opengl3.cpp -c -std=c++20 -m64 -msse3 -Ofast -funroll-loops -Wall -Wextra -Wpedantic -Wno-cpp -I $(SRC) -I $(LIBS) -I $(LIBS)/glad/include -I $(REPS)/ocornut/imgui -I $(LIBS)/glfw-3.3.2/include -I $(REPS)/denis-belov -I $(REPS)/denis-belov/xgk-test/build/gcc-x64/h -I $(REPS)/denis-belov/xgk-math/src -o $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_opengl3.o && objdump -d -M intel -S $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_opengl3.o > $(BUILD)/external/s/$(REPS)/ocornut/imgui/examples/imgui_impl_opengl3.s

$(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_vulkan.o : $(REPS)/ocornut/imgui/examples/imgui_impl_vulkan.cpp 
	mkdir -p $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples && mkdir -p $(BUILD)/external/s/$(REPS)/ocornut/imgui/examples && g++ $(REPS)/ocornut/imgui/examples/imgui_impl_vulkan.cpp -c -std=c++20 -m64 -msse3 -Ofast -funroll-loops -Wall -Wextra -Wpedantic -Wno-cpp -I $(SRC) -I $(LIBS) -I $(LIBS)/glad/include -I $(REPS)/ocornut/imgui -I $(LIBS)/glfw-3.3.2/include -I $(REPS)/denis-belov -I $(REPS)/denis-belov/xgk-test/build/gcc-x64/h -I $(REPS)/denis-belov/xgk-math/src -o $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_vulkan.o && objdump -d -M intel -S $(BUILD)/external/o/$(REPS)/ocornut/imgui/examples/imgui_impl_vulkan.o > $(BUILD)/external/s/$(REPS)/ocornut/imgui/examples/imgui_impl_vulkan.s

$(REPS)/denis-belov/xgk-math/build/gcc-x64/output/a/xgk-math.a : $(REPS)/denis-belov/xgk-math/src/data/const.cpp $(REPS)/denis-belov/xgk-math/src/data/vec4/vec4.cpp $(REPS)/denis-belov/xgk-math/src/data/mat4/mat4.cpp $(REPS)/denis-belov/xgk-math/src/data/mat4/mat4-32.cpp $(REPS)/denis-belov/xgk-math/src/data/mat4/mat4-128.cpp
	make -f /home/denis/reps/denis-belov/xgk-math/makefiles/gcc-x64/makefile

$(REPS)/denis-belov/xgk-aux/build/gcc-x64/output/a/xgk-aux.a : 
	make -f /home/denis/reps/denis-belov/xgk-aux/makefiles/gcc-x64/makefile
